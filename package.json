{
    "name": "common",
    "version": "0.1.0",
    "private": true,
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "src/**/*.js": [
            "yarn lint",
            "git add"
